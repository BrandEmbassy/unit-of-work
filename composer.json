{
  "name": "brandembassy/unit-of-work",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "BrandEmbassy\\UnitOfWork\\": "src/UnitOfWork"
    }
  },
  "require": {
    "php": "~7.2",
    "beberlei/assert": "^3.2",
    "dg/bypass-finals": "^1.1",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "brandembassy/coding-standard": "^5.3",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8",
    "roave/security-advisories": "dev-master"
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs --standard=BrandEmbassyCodingStandard src",
    "phpcbf": "./vendor/bin/phpcbf --standard=BrandEmbassyCodingStandard src",
    "phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon src",
    "test": "./vendor/bin/phpunit src"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "process-timeout": 1200
  }
}
